#import <Foundation/Foundation.h>
#import <DataStruct/Doctor.m>

@interface Appointment: NSObject
{
	Doctor *doctor;
	
	NSDate *date;

	NSString *remark;
    	NSString *precaution;	
    	
    	int status;
    	int appointmentID;
}

@property (retain) Doctor *doctor;

@property (retain) NSDate *date;

@property (retain) NSString *remark;
@property (retain) NSString *precaution;
	    	
@property int status;
@property int appointmentID;

@end
